{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^(vite|@vitejs)(/(.*))?$",
    "^next(/(.*))?$",
    "^clsx(/(.*))?$",
    "<THIRD_PARTY_MODULES>",
    "^@/assets",
    "^@/components",
    "^@/context",
    "^@/dto",
    "^@/hooks",
    "^@/icons",
    "^@/lib",
    "^@/logo",
    "^@/mock",
    "^@/models",
    "^@/providers",
    "^@/types",
    "^@/utils",
    "^@/styles",
    "^(\\.|\\.\\.)/(.(?!.(css|scss)))*$",
    "\\.(css|scss)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderGroupNamespaceSpecifiers": true
}
